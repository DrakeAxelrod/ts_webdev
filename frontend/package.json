{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier './**/*.ts'",
    "prettier:fix": "prettier --write './**/*.ts'",
    "eslint": "eslint './**/*.ts'",
    "eslint:fix": "eslint --fix './**/*.ts'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.ts": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/react": "17.0.30",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-next": "11.1.2",
    "husky": "^7.0.2",
    "lint-staged": "11.2.3",
    "prettier": "2.4.1",
    "typescript": "4.4.4"
  }
}
